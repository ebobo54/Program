set term gif \
    animate \
    optimize \
    delay 100 \
    size 300, 300 \
    background "#ffeedf" \
    crop \
    font "URW Type 1,10"
set output "plot.gif"
set size square
set xlabel "x" 
set ylabel "f(x)"
set xzeroaxis
set grid
set title "График функции f(x)"
plot "< my_graph.txt | tr --delete '$'" with lines title "f(x)"

}

